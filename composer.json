{
    "name": "gpupo-meta/dockerized-helloworld",
    "description": "Dockerize a hello world application",
    "homepage": "https://meta.gpupo.com/dockerized-helloworld/",
    "authors": [
        {
            "name": "Gilmar Pupo",
            "email": "contact@gpupo.com"
        }
    ],
    "keywords": ["docker"],
    "license": "MIT",
    "require": {
        "php": ">=7.3",
        "gpupo/common": "^4.9.2|dev-master",
        "gpupo/common-sdk": "^4.9.2|dev-master",
        "php-amqplib/php-amqplib": "^2.9"
    },
    "require-dev": {
      "phpunit/phpunit": "^8.0",
      "phan/phan": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "Gpupo\\DockerizedHelloworld\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gpupo\\DockerizedHelloworld\\Tests\\": "tests"
        }
    },
    "bin": ["bin/dockerized-helloworld"],
    "scripts": {
    },
    "config": {
        "preferred-install": "dist"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
